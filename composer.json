{
    "name": "schulzefelix/laravel-sistrix",
    "description": "A Laravel 5 package to retrieve Sistrix data.",
    "keywords": [
        "sistrix",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Felix Schulze",
            "homepage": "http://schulze.co",
            "role": "Developer"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit" : "^6.1",
        "mockery/mockery": "^0.9.5",
        "orchestra/testbench" : "~3.4.6"
    },
    "autoload": {
        "psr-4": {
            "SchulzeFelix\\Sistrix\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SchulzeFelix\\Sistrix\\Tests\\": "tests"
        }
    },
    "extra":{
        "laravel":{
            "providers":[
                "SchulzeFelix\\Sistrix\\SistrixServiceProvider"
            ],
            "aliases":{
                "SearchConsole": "SchulzeFelix\\Sistrix\\SistrixFacade"
            }
        }
    }
}
