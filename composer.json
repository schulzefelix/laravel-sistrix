{
    "name": "felixschulze/laravel-sistrix",
    "description": "A Laravel 5 package to retrieve Sistrix data.",
    "keywords": [
        "sistrix",
        "laravel"
    ],
    "homepage": "https://github.com/spatie/laravel-analytics",
    "license": "MIT",
    "authors": [
        {
            "name": "Felix Schulze",
            "homepage": "http://schulze.co",
            "role": "Developer"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "^5.0",
        "orchestra/testbench" : "3.3.x@dev",
        "orchestra/database" : "3.3.x@dev",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
            "Fschulze\\Sistrix\\": "src"
